<SociodemographicPage_Age>:
	BoxLayout:
		orientation: 'vertical'
		MyActionBar:
			title:"Demographics"

		BoxLayout:
			orientation:'vertical'
			pos_hint:{'center_x':0.5}

			Label:
				size_hint_x:0.8
				size_hint_y:0.5
				id:label1
				text_size: root.width*self.size_hint_x, None
				color: TXT_COLOR
				text:"What is your age?"
				halign:'center'
				pos_hint:{'center_x':0.5}

			Label:
				size_hint_x:0.5
				size_hint_y:0.5
				id:age_text_field
				text_size: root.width*self.size_hint_x, None
				color: TXT_COLOR
				text:"< Age >"
				halign:'center'
				pos_hint:{'center_x':0.5}

			GridLayout:
				cols:3
				on_parent:
					for i in range(1,9+1): txt = "{0}".format(i); self.add_widget(button.Button(text = txt,on_release=root.on_numpad_press, background_color= BTN_COLOR, font_style='Button'))
					self.add_widget(button.Button(text = "DEL",on_release=root.on_numpad_press, background_color= BTN_COLOR, font_style='Button'))
					self.add_widget(button.Button(text = "0",on_release=root.on_numpad_press, background_color= BTN_COLOR, font_style='Button'))
					self.add_widget(button.Button(text = "ENTER",on_release=root.on_numpad_press, background_color= BTN_COLOR, font_style='Button'))
		NavButtons

<SociodemographicPage_Sex>:
	BoxLayout:
		orientation: 'vertical'
		MyActionBar:
			title:"Demographics"

		BoxLayout:
			orientation:'vertical'
			pos_hint:{'center_x':0.5}

			Label:
				size_hint_x:0.8
				size_hint_y:0.3
				id:label1
				text_size: root.width*self.size_hint_x, None
				# size: self.texture_size
				color: TXT_COLOR
				text:"What is your sex?"
				halign:'center'
				pos_hint:{'center_x':0.5}

			Widget:
				size_hint_y:0.2

			MyButton2:
				text:"Male"
				size_hint_y:0.5
				on_release:root.button_press(self)
			MyButton2:
				text:"Female"
				size_hint_y:0.5
				on_release:root.button_press(self)

		NavButtons

